.starry-bg-box {
  position: absolute;
  left: -50rpx;
  top: -40rpx;
  height: 110%;
  width: 110%;
  z-index: 0;
}
.starry-bg-box text {
  display: inline-block;
  width: 30px;
  height: 30px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  -webkit-animation: flash 1.5s alternate infinite;
          animation: flash 1.5s alternate infinite;
  position: absolute;
}
@-webkit-keyframes flash {
from {
    opacity: 0;
    -webkit-transform: scale(0, 0);
            transform: scale(0, 0);
}
to {
    opacity: .7;
    -webkit-transform: scale(1, 1);
            transform: scale(1, 1);
}
}
@keyframes flash {
from {
    opacity: 0;
    -webkit-transform: scale(0, 0);
            transform: scale(0, 0);
}
to {
    opacity: .7;
    -webkit-transform: scale(1, 1);
            transform: scale(1, 1);
}
}

